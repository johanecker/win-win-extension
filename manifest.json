{
  "manifest_version": 3,
  "name": "Win Win",
  "version": "1.0.1",
  "description": "Balanced windows for multitasking.",
  "icons": {
    "128": "icon.png"
  },
  "action": {
    "default_popup": "winwin.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "windows", "system.display", "storage"]
}
